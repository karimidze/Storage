<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DomainModel"
                   namespace="DomainModel.Models">

  <class name="UserAccount"  table="Users">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Username" />
    <property name="Password" />

  </class>

  <class name="FileViewModel" table="Files">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="IdFile" />
    <property name="Title" />
    <property name="FileData" type="BinaryBlob" />
    <property name="CreationAuthor" />
    <property name="CreationDate" />
  </class>

  <class name="FileCreateModel" table="Files">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="IdFile" />
    <property name="Title" />
    <property name="FileData" type="BinaryBlob" />
    <property name="CreationAuthor" />
    <property name="CreationDate" />
    
  </class>

</hibernate-mapping>